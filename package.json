{
  "name": "node-op",
  "version": "0.0.0-development",
  "description": "One password CLI installer",
  "keywords": [
    "1-password",
    "one-password",
    "cli",
    "node-op",
    "op"
  ],
  "homepage": "https://github.com/zaripych/node-op#readme",
  "bugs": {
    "url": "https://github.com/zaripych/node-op/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaripych/node-op.git"
  },
  "license": "MIT",
  "author": "@zaripych",
  "files": [
    "dist",
    "cli.js",
    "installOp.js",
    "vaultCheckin.js",
    "vaultCheckout.js",
    "vaultDiff.js",
    "interactive.js",
    "package.json",
    "README.md"
  ],
  "main": "index.js",
  "bin": {
    "node-op": "./cli.js",
    "op": "./dist/binaries/op",
    "vault-checkin": "./vaultCheckin.js",
    "vault-checkout": "./vaultCheckout.js",
    "vault-diff": "./vaultDiff.js"
  },
  "scripts": {
    "before-release": "patch-package && npm run build",
    "prebuild": "node ./scripts/clean.js",
    "build": "node ./scripts/rollup.js -c",
    "build:dev": "node ./scripts/build.js -s",
    "build:watch": "node ./scripts/build.js --watch",
    "check": "concurrently --raw \"node ./scripts/build.js\" \"tsc\" \"tslint -p . -t verbose\"",
    "clean": "node ./scripts/clean.js",
    "combine-coverage": "node ./scripts/combineCoverage.js",
    "postinstall": "node installOp.js",
    "integration": "jest --config ./jest.config.integration.js --color -i",
    "lint": "tslint -p . -t verbose",
    "obtain-fingerprint": "echo -n | openssl s_client -connect cache.agilebits.com:443 | sed -ne '/-BEGIN CERTIFICATE-/,/-END CERTIFICATE-/p' > cert.pem && openssl x509 -noout -in cert.pem -fingerprint",
    "patch": "ts-deps patch",
    "print-current-version": "node ./op-update-watcher/index",
    "release": "node ./scripts/release.js",
    "semantic-release": "semantic-release",
    "test": "jest --color",
    "ts-deps": "ts-deps",
    "type-check": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run check"
    }
  },
  "dependencies": {
    "yoga-layout-prebuilt": "1.9.3"
  },
  "devDependencies": {
    "@babel/preset-react": "7.7.4",
    "@types/rimraf": "2.0.3",
    "@octokit/rest": "17.1.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@types/adm-zip": "0.4.32",
    "@types/ink-spinner": "2.0.3",
    "@types/node": "10.17.13",
    "@types/pump": "1.1.0",
    "@types/react": "16.9.17",
    "@types/tar-fs": "1.16.1",
    "adm-zip": "0.4.13",
    "commander": "4.0.1",
    "fast-glob": "3.1.1",
    "fs-extra": "8.1.0",
    "husky": "4.2.3",
    "ink": "2.7.0",
    "ink-spinner": "3.0.1",
    "ink-text-input": "3.2.1",
    "patch-package": "6.2.0",
    "pump": "3.0.0",
    "react": "16.12.0",
    "rimraf": "3.0.0",
    "rollup": "1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rxjs": "6.5.4",
    "rxjs-marbles": "5.0.4",
    "rxjs-spy": "7.5.1",
    "selenium-webdriver": "3.6.0",
    "semver": "7.1.1",
    "source-map-support": "0.5.16",
    "tar-fs": "2.0.0",
    "ts-deps": "0.12.13",
    "webdriver-query": "1.0.1"
  },
  "op_version": "0.8.0"
}
